<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/general_round_rect" />
            <solid android:color="@color/white" />
            <stroke android:width="0.8dp" android:color="@color/primaryDark" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/general_round_rect" />
            <stroke android:width="0.8dp" android:color="@color/primaryDarker" />
            <solid android:color="@color/primaryDark" />
        </shape>
    </item>
</selector>