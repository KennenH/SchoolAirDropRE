<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <stroke android:width="1px" android:color="@color/primaryDark" />
            <solid android:color="#D7DADF" />
            <corners android:radius="@dimen/general_round_rect" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="1px" android:color="@color/primaryDark" />
            <solid android:color="#80D7DADF" />
            <corners android:radius="@dimen/general_round_rect" />
        </shape>
    </item>
</selector>